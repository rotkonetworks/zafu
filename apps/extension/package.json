{
  "name": "chrome-extension",
  "version": "19.3.0",
  "private": true,
  "license": "(MIT OR Apache-2.0)",
  "description": "chrome-extension",
  "type": "module",
  "scripts": {
    "build": "pnpm bundle:beta && pnpm bundle:prod",
    "bundle:beta": "NODE_OPTIONS=\"--max-old-space-size=8192 --import=./src/utils/webpack-register.js\" webpack --config webpack.beta-config.ts",
    "bundle:prod": "NODE_OPTIONS=\"--max-old-space-size=8192 --import=./src/utils/webpack-register.js\" webpack --config webpack.prod-config.ts",
    "clean": "rm -rfv dist beta-dist bin",
    "dev": "NODE_ENV=testnet pnpm bundle:prod --watch --mode=development -d inline-source-map",
    "lint": "eslint src",
    "lint:strict": "tsc --noEmit && eslint src --max-warnings 0",
    "test": "vitest run"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.10.1",
    "@connectrpc/connect": "^1.7.0",
    "@connectrpc/connect-web": "^1.7.0",
    "@penumbra-labs/registry": "^12.8.0",
    "@penumbra-zone/bech32m": "18.0.0",
    "@penumbra-zone/client": "29.0.0",
    "@penumbra-zone/getters": "28.0.0",
    "@penumbra-zone/keys": "4.2.3",
    "@penumbra-zone/perspective": "61.1.1",
    "@penumbra-zone/protobuf": "11.0.0",
    "@penumbra-zone/query": "workspace:*",
    "@penumbra-zone/storage": "62.0.0",
    "@penumbra-zone/transport-chrome": "9.0.1",
    "@penumbra-zone/transport-dom": "7.5.2",
    "@rotko/penumbra-services": "70.0.0",
    "@rotko/penumbra-types": "37.0.0",
    "@rotko/penumbra-wasm": "55.0.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@repo/context": "workspace:*",
    "@repo/custody-chrome": "workspace:*",
    "@repo/encryption": "workspace:*",
    "@repo/storage-chrome": "workspace:*",
    "@repo/ui": "workspace:*",
    "@repo/wallet": "workspace:*",
    "@repo/zcash-wasm": "workspace:*",
    "@tanstack/react-query": "5.55.0",
    "exponential-backoff": "^3.1.3",
    "framer-motion": "^11.18.2",
    "html5-qrcode": "^2.3.8",
    "immer": "^10.2.0",
    "lodash": "^4.17.23",
    "lucide-react": "^0.439.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-loader-spinner": "^6.1.6",
    "react-router-dom": "^6.30.3",
    "usehooks-ts": "^3.1.1",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@types/chrome": "0.0.270",
    "@types/firefox-webext-browser": "^120.0.5",
    "@types/lodash": "^4.17.23",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/webpack": "^5.28.5",
    "autoprefixer": "^10.4.23",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.3",
    "dotenv": "^16.6.1",
    "eslint": "^9.39.2",
    "html-webpack-plugin": "^5.6.6",
    "postcss": "^8.5.6",
